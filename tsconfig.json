{
    "compilerOptions": {
        "target": "ES2022",
        // "module": "Node16",
        // "moduleResolution": "Node16",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "outDir": ".",
        "rootDir": ".",
        "moduleResolution": "Node",
        "module": "CommonJS"
    },
    "include": [
        "**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}